{
  "entry": "src/index.js",
  "disableCSSModules": false,
  "publicPath": "./",
  "theme": null,
  "autoprefixer": null,
  "proxy": {
    "/api": {
      "target": "http://localhost:14770/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
    }
  },
  "extraBabelPlugins": [
    "transform-runtime",
    ["import", { "libraryName": "antd", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  }
}


